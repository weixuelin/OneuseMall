                android:layout_height="wrap_content">

                <RelativeLayout
                    android:id="@+id/title_bar"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/qb_px_500">

                    <View
                        android:id="@+id/view1"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@color/transparent" />

                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:scaleType="fitXY"
                        android:src="@drawable/my_by" />

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/qb_px_100">

                        <ImageView
                            android:id="@+id/image_my_setting"
                            android:layout_width="@dimen/qb_px_100"
                            android:layout_height="match_parent"
                            android:scaleType="center"
                            android:src="@drawable/my_setting" />

                        <RelativeLayout
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_alignParentRight="true">

                            <ImageView
                                android:id="@+id/image_my_message"
                                android:layout_width="@dimen/qb_px_100"
                                android:layout_height="match_parent"
                                android:scaleType="center"
                                android:src="@drawable/my_message" />

                        </RelativeLayout>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerInParent="true"
                            android:text="个人中心"
                            android:textColor="@color/white"
                            android:textSize="@dimen/qb_px_30" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/qb_px_140">

                        <LinearLayout
                            android:id="@+id/linear_my_edit"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:gravity="center_vertical"
                            android:orientation="horizontal">

                            <ImageView
                                android:layout_width="@dimen/qb_px_140"
                                android:layout_height="@dimen/qb_px_140"
                                android:layout_marginLeft="@dimen/qb_px_20"
