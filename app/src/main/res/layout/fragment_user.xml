                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:layout_marginTop="@dimen/qb_px_10"
                                android:text="待支付"
                                android:textColor="@color/black"
                                android:textSize="@dimen/qb_px_26" />
                        </LinearLayout>

                        <LinearLayout
