                            android:layout_height="wrap_content"
                            android:text="11585.32"
                            android:textColor="@color/orange"
                            android:textSize="@dimen/qb_px_40" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/qb_px_20"
                            android:text="余额"
                            android:textColor="@color/grey_main"
                            android:textSize="@dimen/qb_px_22" />
                    </LinearLayout>

                    <LinearLayout
                        android:paddingRight="@dimen/qb_px_30"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="@dimen/qb_px_30"
                        android:orientation="horizontal">

                        <ImageView
                            android:layout_width="@dimen/qb_px_25"
                            android:layout_height="@dimen/qb_px_25"
                            android:layout_gravity="center_vertical"
                            android:layout_marginRight="@dimen/qb_px_8"
                            android:src="@drawable/user_coupon" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="5"
                            android:textColor="@color/orange"
                            android:textSize="@dimen/qb_px_40" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/qb_px_20"
                            android:text="优惠券"
                            android:textColor="@color/grey_main"
                            android:textSize="@dimen/qb_px_22" />
                    </LinearLayout>
                </RelativeLayout>
            </LinearLayout>

            <TextView
