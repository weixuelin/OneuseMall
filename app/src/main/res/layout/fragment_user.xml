                android:layout_height="@dimen/qb_px_90"
                android:paddingLeft="@dimen/qb_px_20"
                android:paddingRight="@dimen/qb_px_20">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">

                    <ImageView
                        android:layout_width="@dimen/qb_px_30"
                        android:layout_height="@dimen/qb_px_30"
                        android:src="@drawable/user_car" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/qb_px_25"
